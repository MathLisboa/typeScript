{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "lib": ["es5", "dom"],
    "types": ["cypress", "node", "cypress-grep", "./support"]
  },
  "include": ["**/*.ts"]
}
