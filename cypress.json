{
  "reporter": "cypress-multi-reporters",
  "reporterOptions": {
    "configFile": "reporter-config.json"
  },
  "experimentalSourceRewriting": false,
  "defaultCommandTimeout": 10000,
  "viewportWidth": 1366,
  "viewportHeight": 768,
  "video": false,
  "testFiles": "**/*.spec.ts",
  "env": {
    "grepFilterSpecs": true,
    "grepOmitFiltered": true,
    "grepTags": null
  }
}
